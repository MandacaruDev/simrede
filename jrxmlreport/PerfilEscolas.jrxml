     escolas.`inep` AS escola_e_inep,
     escolas.`cep` AS escola_e_cep,
     escolas.`logradouro` AS escola_e_logradouro,
     escolas.`numero` AS escola_e_numero,
     escolas.`complemento` AS escola_e_complemento,
